{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "quotes": ["error", "single"],
      "require-jsdoc": "off",
      "object-curly-newline": "error",
      "object-curly-spacing": "error",
    }
}
